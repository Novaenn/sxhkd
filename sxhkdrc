super + b
	firefox

super + f
	thunar

super + c
	galculator

super + l
	light-locker-command -l

super + v 
	pavucontrol

super + h
	evince ~/Documents/Scripts/keybinds/keybinds.pdf

mod1 + u
	maim --select | xclip -selection clipboard -t image/png

control + End
	poweroff

Print
	maim -s | tee ~/Pictures/Screenshot_$(date +%s).png

XF86MonBrightness{Down,Up}
	brightnessctl set 10%{-,+}

XF86Audio{Lower,Raise}Volume
	pactl set-sink-volume @DEFAULT_SINK@ {-,+}5%

XF86AudioMute
	pactl set-sink-mute @DEFAULT_SINK@ toggle

XF86TouchpadToggle
	toggle-touchpad
